package com.hw.stock.model;

import java.time.LocalDate;
import java.util.Date;

public class Compony {

	public String name;
	public float currentRate;
	public LocalDate highDate;
	public float highRate;
	public LocalDate lowDate;
	public float lowRate;
	public ComponyHistory componyHistory;
	
	public Compony(String name, float currentRate, LocalDate highDate, float highRate, LocalDate lowDate,
			float lowRate) {
		super();
		this.name = name;
		this.currentRate = currentRate;
		this.highDate = highDate;
		this.highRate = highRate;
		this.lowDate = lowDate;
		this.lowRate = lowRate;
	}
	
	public Compony() {
		super();
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public float getCurrentRate() {
		return currentRate;
	}
	
	public void setCurrentRate(float currentRate) {
		this.currentRate = currentRate;
	}
	public LocalDate getHighDate() {
		return highDate;
	}
	public void setHighDate(LocalDate highDate) {
		this.highDate = highDate;
	}
	public float getHighRate() {
		return highRate;
	}
	public void setHighRate(float highRate) {
		this.highRate = highRate;
	}
	public LocalDate getLowDate() {
		return lowDate;
	}
	public void setLowDate(LocalDate lowDate) {
		this.lowDate = lowDate;
	}

	public float getLowRate() {
		return lowRate;
	}
	public void setLowRate(float lowRate) {
		this.lowRate = lowRate;
	}
	@Override
	public String toString() {
		return "Compony [name=" + name + ", CurrentRate=" + currentRate + ", highDate=" + highDate + ", highRate="
				+ highRate + ", lowDate=" + lowDate + ", lowRate=" + lowRate + "]";
	}
}
